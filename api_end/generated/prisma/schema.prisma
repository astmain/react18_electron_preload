generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model user {
  id    Int    @id @default(autoincrement())
  name  String
  email String @unique
}

// generator json {
//   provider = "prisma-json-types-generator"
// }

model tb_history {
  id    Int    @id @default(autoincrement())
  name  String
  url   String
  time1 String
}

model tb_collect {
  id      Int     @id @default(autoincrement())
  name    String
  url     String
  label   String
  is_edit Boolean @default(false)
}

model tb_account {
  id            Int     @id @default(autoincrement())
  platform      String
  platform_name String
  cookies_str   String
  cookies       Json
  local_storage Json
  partition     String  @unique
  nickname      String
  url           String
  user_id       String
  avatar        String
  group_title   String  @default("默认分组")
  line          Boolean @default(false)
  order         Int     @default(0)
}
